{
	"ServiceName": "Imghippo",
	"Title": "ImgHippo",
	"Menu": {
		"Text": "ImgHippo",
		"Icon": "\\uE1AE"
	},
	"Logo": "data:image/jpeg;base64,",
	"DocsUrl": "https://www.imghippo.com/api-info",
	"Description": "Imghippo is a Free Image Hosting platform that allows individuals to upload, share, and manage direct link for free.",
	"VariablePrefix": "IMGHIPPO",
	"Variables": [
		{
			"Key": "ENDPOINT",
			"Type": "String",
			"TargetParam": "Endpoint",
			"InputField": {
				"Label": "Endpoint URL",
				"Type": "Url",
				"IsRequired": true
			}
		},
		{
			"Key": "APIKEY",
			"Type": "String",
			"TargetParam": "ApiKey",
			"InputField": {
				"Label": "Api Key",
				"Type": "Text",
				"ExactLength": 32,
				"IsRequired": true
			}
		}
	],
	"RequestProps": {
		"Endpoint": "https://api.imghippo.com/v1/upload/",
		"FileInfo": {
			"AllowedTypes": ["jpg", "jpeg", "jfif", "png", "gif", "bmp", "webp"],
			"Size": {
				"Minimum": 1,
				"Maximum": 52428800
			}
		},
		"KeyConfig": {
			"Image": {
				"Key": "file",
				"IsRequired": true,
				"UseInRequest": true
			},
			"Api": {
				"Key": "api_key",
				"IsRequired": true,
				"UseInRequest": true
			},
			"Caption": {
				"Key": "title",
				"IsRequired": false,
				"UseInRequest": true
			},
			"Expiration": {
				"Key": "expiration",
				"IsRequired": false,
				"UseInRequest": false
			}
		}
	}
}